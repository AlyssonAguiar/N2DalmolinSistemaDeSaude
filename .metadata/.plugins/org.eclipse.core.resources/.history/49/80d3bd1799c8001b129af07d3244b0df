package model;

public class ResponsavelEmpresarial extends Apolice{

	public int id;
	public String cnpjResponsavel;
	public String nomeResponsavel;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getCnpj() {
		return cnpj;
	}
	public void setCnpj(String cnpj) {
		this.cnpj = cnpj;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	
	public ResponsavelEmpresarial(int id, String cnpj, String nome) {
		super();
		this.id = id;
		this.cnpj = cnpj;
		this.nome = nome;
	}
	

	}
	
	
	
	
